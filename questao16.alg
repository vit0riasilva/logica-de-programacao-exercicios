algoritmo "Questão 16"
// Função : Somatorio
// Autor : Vitória Silva
// Data : 21/06/2021
// Seção de Declarações 
var
   num, soma, i: inteiro
inicio
soma <- 0
i <- 0
      para i de 1 ate 10 faca
           escreval("Digite um número: ")
           leia(num)
           soma <- soma + num
      fimpara

      escreval("A soma dos números digitados é: ", soma)
fimalgoritmo
